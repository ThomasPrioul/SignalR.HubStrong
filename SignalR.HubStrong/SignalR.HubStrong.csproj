<Project Sdk="Microsoft.Net.Sdk">
  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <Authors>Jamie Humphries, Thomas Prioul</Authors>
    <Description>Map and invoke SignalR methods in a strongly typed way on the client and server.</Description>
    <Copyright>Copyright 2017</Copyright>
    <ProjectUrl>https://github.com/ThomasPrioul/SignalR.HubStrong</ProjectUrl>
    <LicenseUrl>https://raw.githubusercontent.com/ThomasPrioul/SignalR.HubStrong/master/LICENSE.md</LicenseUrl>
    <IconUrl>https://raw.githubusercontent.com/ThomasPrioul/SignalR.HubStrong/master/icon.png</IconUrl>
    <DefaultLanguage>en-US</DefaultLanguage>
    <PackageOutputPath>D:\Code\Packages</PackageOutputPath>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.2</Version>
    <Tags>SignalR hub proxy strong strongly typed</Tags>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp"/>
    <PackageReference Include="Castle.Core" Version="4.2.1" />
    <PackageReference Include="Microsoft.AspNet.SignalR.Client" Version="2.2.2" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
  
</Project>
